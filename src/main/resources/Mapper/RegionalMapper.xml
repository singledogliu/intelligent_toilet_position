<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ldq.graduation.design.dao.RegionalMapper">
    <insert id="insert" >
          insert into regional (regional_name,regional_responsible_name)
          values (#{regionalName},#{regionalResponsibleName})
    </insert>
    <delete id="deleteByregionalName">
        delete from regional where regional_code = #{regionalName}
    </delete>
</mapper>